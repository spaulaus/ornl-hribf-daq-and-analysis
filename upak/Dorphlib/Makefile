#
	ranlib orphlib.a
	@echo orphlib.a is up-to-date
#
#
FFLAGS= $(OPT) $(FARGS)
CFLAGS= $(OPT) $(CARGS)
#
.F.o:
	$(F77) $(FFLAGS) -c   $<

.f.o:
	$(F77) $(FFLAGS) -c   $<

.c.o:
	$(CC) $(CFLAGS) -c  $<

install: orphlib
	install -d $(INSTALLDIR)
	install -p orphlib.a $(INSTALLDIR)

clean:
	rm -r *.exe *.o orphlib.a
